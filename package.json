{
  "name": "dragselect",
  "version": "1.9.1",
  "description": "easy javascript drag select functionality for your projects",
  "main": "./dist/ds.min.js",
  "dependencies": {},
  "scripts": {
    "start": "npm install; npm run open & npm run watch",
    "open": "open tests/quicktest.html -a 'Google Chrome'",
    "watch": "tsc --watch",
    "build": "tsc && npm run uglify",
    "uglify": "uglifyjs dist/DragSelect.js -o dist/ds.min.js",
    "deploy": "npm run build && bash deploy.sh",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/puppeteer": "^1.12.2",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "puppeteer": "^1.6.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "uglify-js": "^3.4.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThibaultJanBeyer/DragSelect.git"
  },
  "keywords": [
    "drag",
    "select",
    "plain",
    "js",
    "javascript",
    "vanillajs"
  ],
  "author": "Thibault Jan Beyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThibaultJanBeyer/DragSelect/issues"
  },
  "homepage": "https://ThibaultJanBeyer.com/"
}
